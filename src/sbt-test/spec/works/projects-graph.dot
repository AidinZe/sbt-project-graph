digraph "projects-graph" {
    graph[rankdir="LR"]
    node [
        shape="record"
    ]
    edge [
        arrowtail="none"
    ]
    "foo"[label=<foo>]
    "c"[label=<c>]
    "b"[label=<b>]
    "a"[label=<a>]
    "foo" -> "c" [style=dashed]
    "foo" -> "b" [style=dashed]
    "foo" -> "a" [style=dashed]
    "c" -> "b" [style=solid]
    "b" -> "a" [style=solid]
}
